{
    "manifest_version": 2,
    "name": "Algorithm to Wizard",
    "description": "Just another find and replace: Algorithm to Wizard",
    "version": "1.2",
    "permissions": ["tab"],
    "short_name": "Algorithm to Wizard",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "icons": { "16": "icon16.png",
               "32": "icon32.png",
               "48": "icon48.png",
               "128": "icon128.png"
    }
}
